<!DOCTYPE html>
<html lang="ru">
<head>
  <meta charset="UTF-8">
  <title>Список пользователей</title>
  <style>
    table {
      border-collapse: collapse;
      width: 100%;
    }
    th, td {
      border: 1px solid #ccc;
      padding: 8px;
      text-align: left;
    }
  </style>
</head>
<body>
  <h1>Список пользователей</h1>
  <% if (users && users.length > 0) { %>
    <table>
      <thead>
        <tr>
          <th>Username</th>
          <th>Email</th>
          <th>Watched Videos</th>
          <th>Solved Tasks</th>
          <th>Points</th>
          <th>Level</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach(user => { %>
          <tr>
            <td><%= user.username %></td>
            <td><%= user.email %></td>
            <td><%= user.watchedVideos %></td>
            <td><%= user.solvedTasks %></td>
            <td><%= user.points %></td>
            <td><%= user.level %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  <% } else { %>
    <p>Нет пользователей для отображения.</p>
  <% } %>
</body>
</html>

